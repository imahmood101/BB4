# Server Configuration
PORT=3000
NODE_ENV=development

# Database Configuration
DATABASE_URL=libsql://your-database-url
DATABASE_AUTH_TOKEN=your-auth-token

# Session Configuration
SESSION_SECRET=your-session-secret
